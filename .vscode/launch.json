  "version": "0.2.0",
  "configurations": [ 
    {
      "name": "Launch Chrome",
      "request": "launch",
      "type": "pwa-chrome",
      "url": "http://localhost:8000/test/demo_app.html",
      "webRoot": "${workspaceFolder}"
    },
    {
      "name": "stl_file_test",
      "type": "cppdbg",
      "request": "launch",
      "program": "${fileDirname}/stl_file_test",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${fileDirname}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
